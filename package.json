{
  "name": "stagehand-scraper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "pnpm run migrate && wrangler deploy",
    "migrate": "wrangler d1 migrations apply DB --remote",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "dev": "wrangler dev --local",
    "start": "wrangler dev --local",
    "test": "vitest",
    "test:worker": "python3 tools/test_worker.py",
    "test:schema": "python3 tools/test_schema.py",
    "cf-typegen": "wrangler types"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@cloudflare/vitest-pool-workers": "^0.8.19",
    "@cloudflare/workers-types": "^4.20240725.0",
    "typescript": "^5.5.2",
    "vitest": "~3.2.0",
    "wrangler": "^4.40.2",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.5.0",
    "@cloudflare/playwright": "^1.0.0"
  }
}